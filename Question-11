/*Question-11
		 c_profits gets the buyPrice(int) and sellPrice(int) and determines if there was a profit or loss.
		 it returns a string value that can be "profit","loss","no loss"*/ 
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Please the enter buyPrice");
		int buyPrice = scan.nextInt();
		
		System.out.println("Please the enter sellPrice");
		int sellPrice = scan.nextInt();
		
		System.out.println(c_profits(buyPrice,sellPrice));
		
		
	}

	public static String c_profits(int buyPrice,int sellPrice) {
		
		if(buyPrice<sellPrice){
			
		     return "profit";
		      
		    }else if(buyPrice>sellPrice){
		    	
		      return "loss";
		      
		    }else{
		    	
		      return "no loss";
		    }
			
	}
}
			
			
